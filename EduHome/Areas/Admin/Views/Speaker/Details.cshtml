@model Speaker

<div class="row">
    <div class="col-md-12 grid-margin stretch-card">
        <div class="card">
            <div class="card-body">
                <h4 class="card-title"> Teacher Details</h4>

                <img src="~/assets/img/event/@Model.ImageUrl" />
                <div class="form-group">
                    <label asp-for="FullName">FullName</label>
                    <input asp-for="FullName" class="form-control" disabled accept="image/*">

                </div>
                <div class="form-group">
                    <label asp-for="Profession">Profession</label>
                    <input asp-for="Profession" class="form-control" disabled accept="image/*">
                </div>
                <div class="form-group">
                    <label asp-for="CompanyName">CompanyName</label>
                    <input asp-for="CompanyName" class="form-control" disabled accept="image/*">
                </div>
                


            </div>
        </div>
    </div>
</div>


