@model TeacherCreateViewModel

<div class="row">
    <div class="col-md-12 grid-margin stretch-card">
        <div class="card">
            <div class="card-body">
                <h4 class="card-title"> Create category</h4>
                <form class="forms-sample" method="post" asp-action="Create" enctype="multipart/form-data">
                    <div class="form-group">
                        <div class="form-group">
                            <label asp-for="Image">Name</label>
                            <input asp-for="Image" class="form-control" placeholder="Name" accept="image/*">
                            <span asp-validation-for="Image" class="text-danger"></span>
                        </div>
                        <label asp-for="FullName">FullName</label>
                        <input asp-for="FullName" class="form-control" placeholder="FullName" accept="image/*">
                        <span asp-validation-for="FullName" class="text-danger"></span>
                    </div>
                    <div class="form-group">
                        <label asp-for="Profession">Profession</label>
                        <input asp-for="Profession" class="form-control" placeholder="Profession" accept="image/*">
                        <span asp-validation-for="Profession" class="text-danger"></span>
                    </div>
                    <div class="form-group">
                        <label asp-for="AboutInfo">AboutInfo</label>
                        <input asp-for="AboutInfo" class="form-control" placeholder="AboutInfo" accept="image/*">
                        <span asp-validation-for="AboutInfo" class="text-danger"></span>
                    </div>
                    <div class="form-group">
                        <label asp-for="Degree">Degree</label>
                        <input asp-for="Degree" class="form-control" placeholder="Degree" accept="image/*">
                        <span asp-validation-for="Degree" class="text-danger"></span>
                    </div>
                    <div class="form-group">
                        <label asp-for="Experinence">Experinence</label>
                        <input asp-for="Experinence" class="form-control" placeholder="Experinence" accept="image/*">
                        <span asp-validation-for="Experinence" class="text-danger"></span>
                    </div>
                    <div class="form-group">
                        <label asp-for="Hobbies">Hobbies</label>
                        <input asp-for="Hobbies" class="form-control" placeholder="Hobbies" accept="image/*">
                        <span asp-validation-for="Hobbies" class="text-danger"></span>
                    </div>
                    <div class="form-group">
                        <label asp-for="Faculty">Faculty</label>
                        <input asp-for="Faculty" class="form-control" placeholder="Faculty" accept="image/*">
                        <span asp-validation-for="Faculty" class="text-danger"></span>
                    </div>
                     <div class="form-group">
                        <label asp-for="Mail">Mail</label>
                        <input asp-for="Mail" class="form-control" placeholder="Mail" accept="image/*">
                        <span asp-validation-for="Mail" class="text-danger"></span>
                    </div>
                    <div class="form-group">
                        <label asp-for="PhoneNumber">Phone Number</label>
                        <input asp-for="PhoneNumber" class="form-control" placeholder="Phone Number" accept="image/*">
                        <span asp-validation-for="PhoneNumber" class="text-danger"></span>
                    </div>
                     <div class="form-group">
                        <label asp-for="SkypeAdress">Skype Adress</label>
                        <input asp-for="SkypeAdress" class="form-control" placeholder="Skype Adress" accept="image/*">
                        <span asp-validation-for="SkypeAdress" class="text-danger"></span>
                    </div>
                    <div class="form-group">
                        <label asp-for="Language">Language</label>
                        <input asp-for="Language" class="form-control" placeholder="Language" accept="image/*">
                        <span asp-validation-for="Language" class="text-danger"></span>
                    </div>
                    <div class="form-group">
                        <label asp-for="TeamLeader">TeamLeader</label>
                        <input asp-for="TeamLeader" class="form-control" placeholder="TeamLeader" accept="image/*">
                        <span asp-validation-for="TeamLeader" class="text-danger"></span>
                    </div>
                    <div class="form-group">
                        <label asp-for="Development">Development</label>
                        <input asp-for="Development" class="form-control" placeholder="Development" accept="image/*">
                        <span asp-validation-for="Development" class="text-danger"></span>
                    </div>
                    <div class="form-group">
                        <label asp-for="Design">Design</label>
                        <input asp-for="Design" class="form-control" placeholder="Design" accept="image/*">
                        <span asp-validation-for="Design" class="text-danger"></span>
                    </div>
                    <div class="form-group">
                        <label asp-for="Innovation">Innovation</label>
                        <input asp-for="Innovation" class="form-control" placeholder="Innovation" accept="image/*">
                        <span asp-validation-for="Innovation" class="text-danger"></span>
                    </div>
                    <div class="form-group">
                        <label asp-for="Communication">Communication</label>
                        <input asp-for="Communication" class="form-control" placeholder="Communication" accept="image/*">
                        <span asp-validation-for="Communication" class="text-danger"></span>
                    </div>
                 


                    <button  type="submit" class="btn btn-primary mr-2">Submit</button>
                    <button asp-controller="Teacher" asp-action="Index" class="btn btn-light">Cancel</button>
                </form>
            </div>
        </div>
    </div>
</div>


